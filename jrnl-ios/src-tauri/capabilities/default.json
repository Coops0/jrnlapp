{
  "$schema": "../gen/schemas/iOS-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "store:default",
    "store:allow-load",
    "http:default",
    "fs:default",
    "fs:create-app-specific-dirs",
    "fs:read-app-specific-dirs-recursive",
    "fs:allow-appcache-meta",
    "fs:allow-appcache-meta-recursive",
    "fs:allow-appcache-read",
    "fs:allow-appcache-read-recursive",
    "fs:allow-appcache-write",
    "fs:allow-appcache-write-recursive",
    "sign-in-with-apple:default",
    "google-signin:default"
  ]
}