{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jrnl-ios",
  "version": "0.1.0",
  "identifier": "fm.jrnl.jrnl-ios-app",
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run generate",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "jrnl-ios",
        "width": 800,
        "height": 600
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
