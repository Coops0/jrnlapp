{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jrnl",
  "version": "0.1.0",
  "identifier": "fm.jrnl.jrnl-app-id",
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run generate",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "jrnl",
        "maximized": true,
        "userAgent": "Mozilla/5.0 (iPhone; CPU iPhone OS 18_1 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Version/18.1 Safari/605.1.15",
        "backgroundColor": "#000000"
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "developmentTeam": "X4FSN5HD4U"
    }
  }
}
